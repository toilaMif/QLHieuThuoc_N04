package entity;

import java.util.Objects;

public class ent_taiKhoan {
		 private String maTK;
		 private String maKhau;
		 private String tenDangNhap;
		 private String chucVu;
		
		public String getMaTK() {
			return maTK;
		}
		public void setMaTK(String maTK) {
			this.maTK = maTK;
		}
		public String getMaKhau() {
			return maKhau;
		}
		public void setMaKhau(String maKhau) {
			this.maKhau = maKhau;
		}
		public String getTenDangNhap() {
			return tenDangNhap;
		}
		public void setTenDangNhap(String tenDangNhap) {
			this.tenDangNhap = tenDangNhap;
		}
		public String getChucVu() {
			return chucVu;
		}
		public void setChucVu(String chucVu) {
			this.chucVu = chucVu;
		}
		public ent_taiKhoan() {
			super();
			// TODO Auto-generated constructor stub
		}
		public ent_taiKhoan(String maTK, String maKhau, String tenDangNhap, String chucVu) {
			super();
			this.maTK = maTK;
			this.maKhau = maKhau;
			this.tenDangNhap = tenDangNhap;
			this.chucVu = chucVu;
		}
		public ent_taiKhoan(String maTK) {
			super();
			this.maTK = maTK;
		}
		@Override
		public int hashCode() {
			return Objects.hash(maTK, tenDangNhap);
		}
		@Override
		public boolean equals(Object obj) {
			if (this == obj)
				return true;
			if (obj == null)
				return false;
			if (getClass() != obj.getClass())
				return false;
			ent_taiKhoan other = (ent_taiKhoan) obj;
			return Objects.equals(maTK, other.maTK) && Objects.equals(tenDangNhap, other.tenDangNhap);
		}
		
		
		
		
		
		 
		 
}
